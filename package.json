{
  "name": "compan",
  "version": "0.0.0",
  "description": "All-in-one set of organizational tools.",
  "author": "James Pietras <james.m.pietras@gmail.com>",
  "license": "UNLICENSED",
  "main": "app.js",
  "private": true,
  "homepage": "https://github.com/jamespietras/compan#readme",
  "keywords": [
    "organizational",
    "utility"
  ],
  "contributors": [
    {
      "name": "James Pietras",
      "email": "james.m.pietras@gmail.com"
    }
  ],
  "scripts": {
    "addons": "npm run typings && npm run check",
    "check": "npm run prefix && echo 'Checking dependencies...' && nsp check --output checkstyle || true && echo ''",
    "cleanup": "npm run prefix && echo 'Cleaning up...' && rm -rf build node_modules typings && echo ''",
    "compress": "npm run prefix && echo 'Compressing git repository...' && git gc || true",
    "development": "concurrently --kill-others --raw 'webpack --watch --config webpack.dev.babel.js' 'pm2 logs --raw'",
    "prefix": "echo '\n\\033[1;97;104m[compan]\\033[0m \\c'",
    "preinstall": "npm run compress",
    "postinstall": "npm run resolve-confirmation && npm run addons",
    "resolve-confirmation": "npm run prefix && echo 'Dependencies resolved.'",
    "typings": "npm run prefix && echo 'Installing typings...' && typings install"
  },
  "dependencies": {
    "@types/lodash": "^4.14.55",
    "babel-core": "^6.24.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "bytes": "^2.4.0",
    "chalk": "^1.1.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "helmet": "^3.5.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.0",
    "pm2": "^2.4.2",
    "prettyjson": "^1.2.1",
    "pug": "^2.0.0-beta11",
    "source-map-support": "^0.4.12",
    "ts-loader": "^2.0.1",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1",
    "webpack-node-externals": "^1.5.4",
    "winston": "^2.3.1",
    "winston-mongodb": "^2.0.8"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.16",
    "concurrently": "^3.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "nsp": "^2.6.3",
    "webpack-shell-plugin": "^0.5.0"
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamespietras/compan.git"
  },
  "bugs": {
    "url": "https://github.com/jamespietras/compan/issues"
  },
  "engines": {
    "node": "6.10.0",
    "npm": "3.10.10"
  }
}
